{
    "manifest_version": 3,
    "name": "Database Manager Extension",
    "description": "Automatically creates a new database when the current one expires.",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  